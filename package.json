{
  "name": "phagedirectory",
  "version": "1.1.0",
  "description": "Phage Directory app",
  "author": "janzheng <janeazy@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .eslintignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .eslintignore .",
    "precommit": "npm run lint",
    "commit": "npm run precommit; git add . -A ; git commit ; git push origin master ",
    "dirty": "git add . -A ; git commit ; git push origin master ",
    "dev": "NODE_ENV=spa nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "cd dist; surge . -d https://phage.directory; say 'prod ready'",
    "stagedeploy": "cd dist; surge . -d v2.phagedirectory.surge.sh; say 'stage ready'",
    "stage": "NODE_ENV=spa npm run generate --spa; npm run stagedeploy",
    "stagebuild": "npm run generate; npm run stagedeploy",
    "prod": "npm run generate; npm run deploy",
    "prodspa": "NODE_ENV=spa npm run generate --spa; npm run deploy"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/google-tag-manager": "^2.1.0",
    "@nuxtjs/manifest": "^2.6.0",
    "@nuxtjs/markdownit": "^1.2.2",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/workbox": "^2.6.0",
    "axios": "^0.18.0",
    "coeur": "^0.2.6",
    "cross-env": "^5.2.0",
    "cytosis": "^0.0.306",
    "dayjs": "^1.7.7",
    "markdown-it": "^8.4.2",
    "markdown-it-attrs": "^2.3.2",
    "node-sass": "^4.10.0",
    "nuxt": "^2.4.3",
    "sass-loader": "^7.0.3",
    "vue-disqus": "^3.0.5",
    "vue-scrollto": "^2.11.0",
    "vuelidate": "^0.7.4",
    "vuex-cache": "^1.3.0",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-typeof-symbol": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-loader": "^8.0.4",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "lodash": "^4.17.19",
    "nodemon": "^1.11.0",
    "webpack-stats-plugin": "^0.2.1"
  }
}
